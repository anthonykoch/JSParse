{
  "private": true,
  "name": "blade-expression",
  "version": "1.0.0",
  "description": "A parser for JavaScript expressions",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf dist/ && rm -rf build/",
    "test": "node \"./test\" | tap-spec || exit 0",
    "test-diff": "node \"./test\" | tap-diff || exit 0",
    "build:regex": "node ./regex.js > lib/constants/regex.js",
    "build:package": "babel ./lib --out-dir build/package",
    "build:split": "npm run build:regex && webpack --config webpack.config.split.js",
    "build:js": "webpack --config webpack.config.js",
    "build": "npm run build:regex && npm run build:package && npm run build:js && npm run build:split",
    "watch:js": "npm run build:js -- --watch",
    "minify": "bash bin/minify.sh"
  },
  "repository": {
    "type": "git"
  },
  "directories": {
    "test": "test"
  },
  "author": "anthonykoch",
  "license": "MIT",
  "dependencies": {
    "object-assign": "^4.1.0",
    "pad-start": "^1.0.2",
    "repeat-string": "^1.5.4"
  },
  "devDependencies": {
    "acorn": "^4.0.3",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-es2015-unicode-regex": "^6.11.0",
    "babel-preset-es2015": "^6.16.0",
    "babili": "0.0.7",
    "glob": "^7.0.3",
    "tap-diff": "^0.1.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2",
    "webpack": "^1.13.2",
    "yargs": "^6.0.0"
  }
}

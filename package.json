{
  "private": true,
  "name": "blade-expression",
  "version": "1.0.0",
  "description": "A parser for JavaScript expressions",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint lib/**/*.js test/**/*.spec.js test/*.js",
    "serve": "browser-sync start -c .browsersync.js",
    "clean": "rm -rf dist/ && rm -rf build/",
    "test": "node \"./test\" | tap-spec || exit 0",
    "test-diff": "node \"./test\" | tap-diff || exit 0",
    "build:regex": "node ./regex.js > lib/constants/regex.js",
    "build:package": "npm run build:regex && babel ./lib --out-dir build/package",
    "build:split": "npm run build:regex && webpack --config webpack.config.split.js",
    "build:js": "npm run build:regex && webpack --config webpack.config.js",
    "build": "npm run build:regex && npm run build:package && npm run build:js && npm run build:split",
    "watch:js": "npm run build:js -- --watch",
    "minify": "npm run clean && npm run build:js && babili dist/bladeexp.js > dist/bladeexp.min.js",
    "cov": "npm run minify && istanbul cover ./test/index.js -i lib/**/*.js"
  },
  "repository": {
    "type": "git"
  },
  "directories": {
    "test": "test"
  },
  "author": "anthonykoch",
  "license": "MIT",
  "dependencies": {
    "object-assign": "4.1.x",
    "pad-start": "1.0.x",
    "repeat-string": "1.5.x"
  },
  "devDependencies": {
    "acorn": "5.0.x",
    "babel-core": "6.24.x",
    "babel-loader": "6.2.x",
    "babel-plugin-transform-es2015-unicode-regex": "6.22.x",
    "babel-preset-es2015": "6.16.x",
    "babili": "0.0.x",
    "browser-sync": "^2.18.8",
    "dedent-js": "1.0.x",
    "eslint": "^3.19.0",
    "front-matter": "2.1.x",
    "glob": "7.0.x",
    "istanbul": "0.4.x",
    "tap-diff": "0.1.x",
    "tap-spec": "4.1.x",
    "tape": "4.6.x",
    "webpack": "1.13.x"
  }
}
